<!-- DEPRECATED -->
<% count = -1 %>
<%= f.fields_for :pin_images do |ff| %>
<div id='<%= ff.object.id %>' class='well form-inline'>
  <% count += 1 %>
  <script type="text/javascript">
  $('input[id=pin_pin_images_attributes_<%= count %>_photo]').change(function() {
  $('#photoCover<%= count %>').val($(this).val());
  });
  </script>
  <%= render partial: 'pin_images/persisted', locals: {ff: ff} if ff.object.persisted? %>
  <div class="input-append">
    <%= ff.file_field(:photo, accept:'image/png,image/jpg,image/jpeg', class: 'hidden') %>
    <input id="photoCover<%= count %>" class="form-control" type="text" placeholder="Browse for Image">
    <a class="btn btn-default" onclick="$('input[id=pin_pin_images_attributes_<%= count %>_photo]').click();">Browse</a>
  </div>
  <div class="form-group">
    <div class="input-group">
      <label class="sr-only" for="caption">Caption</label>
      <%= ff.text_field :caption, class: "form-control", placeholder: "Caption" %>
    </div>
  </div>
  <% end %>