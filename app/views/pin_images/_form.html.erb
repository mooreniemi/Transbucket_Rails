<div class="fallback">
  <div id="persisted_pins">

  </div>
  <div>
    <div class="header">
      <ul class="nav nav-pills pull-right">
        <li class="active">
        </li>
      </ul>
      <h3 class="text-muted">Add new photos</h3>
    </div>
    <div class="dropzone" id="dropper">
      <div class="fallback">
        <%= form_for(PinImage.new, html: {multipart: true}) do |pin_image_form| %>
        <%= pin_image_form.file_field :photo, multiple: true, name: "pin_image[photo]" %>
        <%= pin_image_form.text_field :caption %>
        <% end %>
      </div>
    </div>
  </div>
</div>