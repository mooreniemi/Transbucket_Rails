    </div>

  </div>

  <br>
  <br clear="both" />

  <div class="page_container" class="col-md-6">
    <%= will_paginate @pins unless @pins.nil? %>
  </div>
  <script>
  $("#scope").multiselect({
     position: {
        my: 'bottom',
